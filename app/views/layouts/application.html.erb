<!DOCTYPE html>
<html>
<head>
  <title>Steve</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag "jquery","jquery-timers","jquery-ui","application" %>
  <script>
  $(function(){
    var game = new Game();
    game.initialize();
    game.show_hud("results");
  });
  </script>
  <%= csrf_meta_tag %>
</head>
<body>

<script>
  window.fbAsyncInit = function() {
    FB.init({appId: '123348717688455', status: true, cookie: true, xfbml: true});
  };
  (function() {
    var e = document.createElement('script'); e.async = true;
    e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
    document.getElementById('fb-root').appendChild(e);
  }());
</script>

<%= yield %>

</body>
</html>
